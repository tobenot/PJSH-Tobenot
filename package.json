{
    "name": "Commmand-Combat",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:itch": "node scripts/build-itch.js",
        "build:pages": "cross-env GITHUB_PAGES=true vite build",
        "test:env": "node scripts/test-env.js",
        "deploy": "npm run build:pages && gh-pages -d dist",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest",
        "coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@tobenot/basic-web-game-backend-contract": "^1.1.4",
        "@trpc/client": "^11.4.3",
        "@trpc/server": "^11.4.3",
        "@types/react-router-dom": "^5.3.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^7.6.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^14.3.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/archiver": "^6.0.3",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "archiver": "^7.0.1",
        "autoprefixer": "^10.4.20",
        "cross-env": "^7.0.3",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "gh-pages": "^6.2.0",
        "jsdom": "^22.1.0",
        "postcss": "^8.4.49",
        "tailwindcss": "^3.4.16",
        "typescript": "^5.0.2",
        "vite": "^4.4.5",
        "vitest": "^0.34.6"
    }
}
